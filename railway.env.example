# Railway Environment Variables Example
# Скопируйте этот файл и настройте переменные в Railway Dashboard

# Telegram Bot
TELEGRAM_BOT_TOKEN=your_bot_token_here
ALLOWED_USERS=5015100177,123456789

# Database
DATABASE_URL=postgresql://user:password@host:port/database

# YooKassa (для продакшена)
YOOKASSA_SHOP_ID=your_shop_id
YOOKASSA_SECRET_KEY=your_secret_key

# OpenAI (для GPT функций)
OPENAI_API_KEY=your_openai_api_key

# Google Cloud (для Veo и TryOn)
GCP_KEY_JSON_B64=your_base64_encoded_service_account_key

# Flask
FLASK_DEBUG=False
PORT=8000

# Другие настройки
NANO_API_KEY=your_nano_api_key
BG_REMOVAL_API_KEY=your_bg_removal_api_key

